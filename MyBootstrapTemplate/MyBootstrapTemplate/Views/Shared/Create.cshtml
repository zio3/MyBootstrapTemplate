@{
    ViewBag.Title = " 新規追加";
    var returnUrl = ViewBag.__ReturnUrl as string;
}

<h2>新規追加</h2>


@Html.ValidationSummary(false)
<hr />
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        @Html.Hidden("__ReturnUrl")
        @Html.EditorForModel()
        <div class="form-group">
            <div class="col-sm-offset-2 col-sm-10">
                <button type="submit" class="btn btn-primary">編集を適用</button>
                @Html.Partial("Parts/ReturnLinkButton")
            </div>
        </div>
    </div>
}
@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
